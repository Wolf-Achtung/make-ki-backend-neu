ENV=production
APP_VERSION=2025.10
LOG_LEVEL=INFO

# CORS: you may use JSON (recommended) or CSV. Empty value defaults to "*".
# Example JSON list:
# CORS_ALLOW_ORIGINS_RAW=["https://ki-sicherheit.jetzt","http://localhost:5173"]
# Or CSV:
# CORS_ALLOW_ORIGINS_RAW=https://ki-sicherheit.jetzt,http://localhost:5173
# Or allow any:
CORS_ALLOW_ORIGINS_RAW=*

# Feature flags
QUEUE_ENABLED=false
PDF_SERVICE_ENABLED=true

# External services
# PDF_SERVICE_URL=https://ki-pdf-service.example.com/render